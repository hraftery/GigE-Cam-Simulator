<memory>
	<!-- // Bootstrap Registers first -->
	<property>
		<register>Manufacturer_name</register>
		<string>Teledyne</string>
	</property>
	<property>
		<register>Model_name</register>
		<string>Linea M8192 sim</string>
	</property>
	<property>
		<register>Device_version</register>
		<string>v0.1</string>
	</property>
	<property>
		<register>Manufacturer_specific_information</register>
		<string>my fake cam</string>
	</property>
	<property>
		<register>Serial_number</register>
		<string>0123456789</string>
	</property>
	<property>
		<register>User_defined_name</register>
		<string>The quick brown fox jumps over a lazy dog</string>
	</property>
	<property>
		<register>GVCP_Capability</register>
		<bit>0</bit>  <!-- // User-defined name register is supported. -->
		<bit>1</bit>  <!-- // Serial number register is supported -->
		<bit>2</bit>  <!-- // Heartbeat disabled -->
<?ignore
		<bit>30</bit> <!-- // WRITEMEM_CMD and WRITEMEM_ACK are supported. -->
?>
		<bit>31</bit> <!-- // concatenation (multiple operations in one message) is supported. -->
	</property>
	
	<!-- // https://github.com/AravisProject/aravis/blob/eb948f080775f9ff78ad6d1fa053aff9e5e2abc9/src/arvfakecamera.c#L1076 -->
	<property>
		<register>Number_of_Stream_channels</register>
		<int>1</int>
	</property>
	<property>
		<register>Stream_Channel_Packet_Size_0</register>
		<int>1080</int>
	</property>
	
	<!-- // Manufacturer specific registers follow -->
	<property>
		<register>0x20003490</register>  <!-- // deviceManufacturerPartNumber  -->
		<string>FAKECAM1234</string>
	</property>
	<property>
		<register>0x18009320</register>  <!-- // DeviceManufacturerInfo  -->
		<string>Simulated camera</string>
	</property>
	<property>
		<register>0x180000E0</register>  <!-- // DeviceFirmwareVersion  -->
		<string>v0.1f</string>			 <!-- // In a simulated device, the device version *is* the software version. Just add 'f' suffix.  -->
	</property>
	<property>
		<register>0x20000060</register>  <!-- // Pixel Format  -->
		<int>0x01080001</int>            <!-- // Mono8  -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x200001C0</register>  <!-- // PixelFormat capability register  -->
		<bit>0</bit>  <!-- // isPixelFormatMono8Implemented -->
<?ignore
		<bit>1</bit>  <!-- // isPixelFormatMono10Implemented -->
		<bit>2</bit>  <!-- // isPixelFormatMono12Implemented -->
		<bit>3</bit>  <!-- // isPixelFormatBayer8Implemented -->
		<bit>4</bit>  <!-- // isPixelFormatBayer10Implemented -->
		<bit>5</bit>  <!-- // isPixelFormatBayer12Implemented -->
		<bit>6</bit>  <!-- // isPixelFormatBGRa8Implemented -->
		<bit>7</bit>  <!-- // isPixelFormatBGRY8Implemented -->
		<bit>8</bit>  <!-- // isPixelFormatRGB10p32Implemented -->
		<bit>9</bit>  <!-- // isPixelFormatYUV422_8_UYVYImplemented -->
		<bit>10</bit> <!-- // isPixelFormatYUV422_8_YUYVImplemented -->
		<bit>11</bit> <!-- // isPixelFormatMono12PackedImplemented -->
		<bit>12</bit> <!-- // isPixelFormatGreenOnly8Implemented -->
		<bit>13</bit> <!-- // isPixelFormatRGBG8Implemented -->
		<bit>14</bit> <!-- // isPixelFormatRGBG12Implemented -->
		<bit>15</bit> <!-- // isPixelFormatRGBG12PackedImplemented -->
		<bit>16</bit> <!-- // isPixelFormatRGB12V1Implemented -->
		<bit>17</bit> <!-- // isPixelFormatRGB8PackedImplemented -->
?>
		<endianess>LittleEndian</endianess>
	</property>	
	
<?ignore
	<property>
		<register>0x20000E50</register>  <!-- // transferTurbo Pixel Format CapabilityValue Registers  -->
		<bit>0</bit>  <!-- // capMono8 -->
		<bit>2</bit>  <!-- // capMono12 -->
		<bit>12</bit> <!-- // capG8 -->
		<bit>13</bit> <!-- // capBiColorRGBG8 -->
		<bit>14</bit> <!-- // capBiColorRGBG12 -->
		<endianess>LittleEndian</endianess>
	</property>
?>

	<property>
		<register>0x200000F0</register>  <!-- // SensorWidth  -->
		<int>8192</int>
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000100</register>  <!-- // SensorHeight  -->
		<int>1</int>
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x200001D0</register>  <!-- // AcquisitionLineRate  -->
		<int>4000000</int>				 <!-- // In 0.001Hz -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000090</register>  <!-- // Height  -->
		<int>480</int>
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000070</register>  <!-- // Width  -->
		<int>640</int>
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x2000009C</register>  <!-- // SaperaBufferFormat  -->
		<int>16844800</int>
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000058</register>  <!-- // AcquisitionFrameCountMax  -->
		<int>20</int>
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000E10</register>  <!-- // triggerFrameCount  -->
		<int>1</int>					 <!-- // Must be in [1,4095]. -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000570</register>  <!-- // triggerLineCount  -->
		<int>1</int>					 <!-- // Must be in [1,15]. -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000C20</register>  <!-- // lineTriggerInputFrequency  -->
		<int>4294967295</int>			 <!-- // Seems to be stored as period in 10's of ns, so zero blows up. Pick UINT32_MAX. -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x08000000</register>  <!-- // UserSetDefaultSelector ("Power-up Configuration Selector")  -->
		<int>6</int>					 <!-- // Factory Setting values  -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x200001B0</register>  <!-- // Camera Temperature / FPGA_Board. In degrees Celsius  -->
		<int>35</int>
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000340</register>  <!-- // FlatfieldCalibrationSampleSizeReg  -->
		<int>2048</int>					 <!-- // Lines_2048  -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x200032D0</register>  <!-- // cP_flatfieldCorrectionCurrentActiveSetReg ("current preset")  -->
		<int>1</int>					 <!-- // User Flat Field 1  -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000E68</register>  <!-- // flatfieldAlgorithmGainMax = 0x20000E60 + 4 * REG_MAX_INDEX  -->
		<int>409600</int>			     <!-- // Is converted to float by /4096 so this gives max of 100. Typical gain seems to be 7.5  -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20000278</register>  <!-- // gainRawMax = 0x20000270 + 4 * REG_MAX_INDEX  -->
		<int>409600</int>			     <!-- // Same as flatfieldAlgorithmGainMax -->
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20002EEC</register>  <!-- // Binning Horizontal  -->
		<int>1</int>
		<endianess>LittleEndian</endianess>
	</property>
	<property>
		<register>0x20002EFC</register>  <!-- // Binning Veritical  -->
		<int>1</int>
		<endianess>LittleEndian</endianess>
	</property>
</memory>